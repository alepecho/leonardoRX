@model IEnumerable<SIMAMUS.GUI.Models.TipoConsulta>

@{
    ViewBag.Title = "IndexAdministrador";
    Layout = "~/Views/Shared/_LayoutAdministrador.cshtml";
}

<h2>Informción general de los tipos de consulta</h2>

<p>
    @Html.ActionLink("Registrar", "CreateAdministrador")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.NombreConsulta)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.NombreConsulta)
        </td>
        <td>
            @Html.ActionLink("Editar", "EditAdministrador", new { id=item.IdTipoConsulta }) |
            @Html.ActionLink("Ver Detalles", "DetailsAdministrador", new { id=item.IdTipoConsulta })
        </td>
    </tr>
}

</table>
